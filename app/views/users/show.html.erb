<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%=@user.name%></title>
  </head>
  <body>
    <h4>Current user page</h4>
    <ul>
      <li>name: <%=@user.name%></li>
      <li>email: <%=@user.email%></li>
      <li><%= link_to "edit", edit_user_registration_path %></li>
    <ul>

    <div class="">
      <ul>
        <h3>user's tickers</h3>
        <% @user.stocks.all.each do |stock| %>
        <li><%= stock.ticker %></li>
        <% end %>
      </ul>
    </div>
  </body>
</html>
